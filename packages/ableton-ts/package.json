{
  "name": "@aptrn/ableton-ts",
  "version": "0.0.1",
  "description": "Typescript library to interact with Ableton Live Object Model",
  "main": "dist/index.js",
  "module": "dist/index.esm.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "lint": "tsc",
    "test": "jest",
    "build": "rollup -c rollup.config.js",
    "dev": "rollup -w -c rollup.config.js",
    "docs": "pnpx typedoc",
    "type-check": "tsc --noEmit"
  },
  "files": [
    "dist",
    "README.md"
  ],
  "exports": {
    ".": {
      "default": "./dist/index.js",
      "types": "./dist/index.d.ts",
      "map": "./dist/index.js.map"
    }
  },
  "keywords": [
    "maxmsp",
    "maxforlive",
    "max4live",
    "m4l",
    "ableton live",
    "ableton"
  ],
  "author": "aptrn",
  "license": "MIT",
  "dependencies": {
    "@types/maxmsp": "^1.0.13",
    "@rollup/plugin-commonjs": "^28.0.0",
    "@rollup/plugin-node-resolve": "^15.3.0",
    "@rollup/plugin-terser": "^0.4.4",
    "@rollup/plugin-typescript": "^12.1.0",
    "@types/jest": "^29.5.13",
    "@types/node": "^22.7.4",
    "jest": "^29.7.0",
    "rollup": "^4.24.0",
    "ts-jest": "^29.2.5",
    "tslib": "^2.7.0",
    "typedoc": "^0.26.8",
    "typescript": "^5.6.2"
  }
}
